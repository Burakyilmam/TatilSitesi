@using TatilSitesi.Models
@model IEnumerable<HotelRoomImage>

@foreach (var item in Model)
{
    <div class="col-12">
        <div style="float:left">
            <img width="100%" height="800" style="padding:10px;margin-left:-140px;margin-top:-10px" src="@item.HotelRoomImageUrl">
        </div>
    </div>
}